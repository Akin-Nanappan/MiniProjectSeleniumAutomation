<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="3" verbose="1" name="Hackathon Zigwheels.com Smoke Test"
	annotations="JDK" parallel="tests">
	<test name="Upcoming Honda Bikes">
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="smokeTest"></include>
			</run>
		</groups>
		<classes>
			<class name="TestCases.UpcomingHondaBikes"></class>
		</classes>
	</test>
	<test name="Popular Models in Used Cars In Chennai">
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="smokeTest"></include>
			</run>
		</groups>
		<classes>
			<class name="TestCases.PopularModelsInChennai"></class>
		</classes>
	</test>
	<test name="Invalid Google Login Test">
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="smokeTest"></include>
			</run>
		</groups>
		<classes>
			<class name="TestCases.InvalidLoginTest"></class>
		</classes>
	</test>
</suite>